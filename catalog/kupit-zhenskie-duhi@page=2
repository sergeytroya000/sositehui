$(document).ready(function(){
   Height = '';
   maxHeight = 0;
   $(".product").each(function(){
     if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
     Height = maxHeight;
   });
   $(".product").height(Height);
});
